# Environment Variables for Identity Service
# Copy this file to .env and update with your actual values

# Database Configuration
DB_HOST=localhost
DB_PORT=5001
DB_USER=postgres
DB_PASSWORD=root
DB_NAME=postgres
DB_SSL_MODE=disable

# JWT Configuration
JWT_
JWT_EXPIRATION=15m

# Server Configuration
SERVER_PORT=8080
SERVER_HOST=localhost

# Log Configuration
LOG_LEVEL=info

# Redis Configuration (if you plan to use caching)
REDIS_HOST=localhost
REDIS_PORT=6001
REDIS_PASSWORD=

# Service URLs
CART_SERVICE_URL=http://localhost:8082

# Environment
ENV=development
